{
	"private": true,
	"version": "0.0.0",
	"publisher": "scottwillmoore",
	"name": "experiments",
	"displayName": "Scott's Experiments",
	"description": "Scott's incubator for new extension features and ideas",
	"license": "MIT",
	"main": "./outputs/extension.js",
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"commands": [
			{
				"command": "scottwillmoore.experiments.run",
				"title": "Run Experiment"
			}
		]
	},
	"scripts": {
		"build": "tsc",
		"check": "prettier --check .",
		"fix": "prettier --write .",
		"watch": "tsc --watch"
	},
	"engines": {
		"node": "^18.5.0",
		"npm": "^9.0.0",
		"vscode": "^1.82.0"
	},
	"devDependencies": {
		"@types/node": "^18.5.0",
		"@types/vscode": "^1.82.0",
		"prettier": "^3.0.0",
		"typescript": "^5.2.0"
	}
}
